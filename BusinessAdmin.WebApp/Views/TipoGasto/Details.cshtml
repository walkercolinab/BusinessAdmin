@model BusinessAdmin.WebApp.Models.TipoGasto

@{
    ViewBag.Title = "Detalle de tipo de gasto: " + @Model.Nombre;
}

<div class="box">
    <br />
    <dl class="dl-horizontal">
        <dt class="form-detail">
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Nombre)
        </dd>

        <dt class="form-detail hide">
            @Html.DisplayNameFor(model => model.EsActivo)
        </dt>

        <dd class="hide">
            @Html.DisplayFor(model => model.EsActivo)
        </dd>

    </dl>

    <div class="paddingLeft15">
        @Html.ActionLink("Editar", "Edit", new { id = Model.TipoGastoID })<span> | </span>

        @Html.ActionLink("Volver a Lista de Tipos de Gasto", "Index")
    </div>
    <br />
</div>
